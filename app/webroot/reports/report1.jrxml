	<queryString>
		<![CDATA[select
p.nome as pais_nome,
e.nome as estado_nome,
c.nome as cidade_nome
from

paise as p,
estado as e,
cidade as c

where e.id = (select estado_id from cidade where id = $P{CIDADE_ID} )
AND   e.id = c.estado_id
AND   p.id = e.pais_id]]>
	</queryString>
