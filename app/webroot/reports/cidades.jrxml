	<queryString>
		<![CDATA[SELECT
p.nome as pais_nome,
e.nome as estado_nome,
c.nome as cidade_nome
FROM

paise as p,
estado as e,
cidade as c

WHERE e.id = (select estado_id FROM cidade WHERE id = CAST( $P{CIDADE_ID} AS UNSIGNED ) )
AND   e.id = c.estado_id
AND   p.id = e.pais_id
]]>
	</queryString>
